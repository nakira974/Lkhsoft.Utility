<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>Lkhsoft.Utility</Title>
        <Authors>nakira974</Authors>
        <Description>Contain useful ASP.NET.Core services and others definitions</Description>
        <PackageIcon>random_001.jpg</PackageIcon>
